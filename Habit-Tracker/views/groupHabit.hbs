
<h1>{{title}}</h1>

{{!-- HABIT CURRENT WEEK TABLE FOR CURRENT USER--}}
<table class="table caption-top w-50 weekly-progress" >
  <thead>
    <tr>
      <th scope="col-1">M</th>
      <th scope="col-1">T</th>
      <th scope="col-1">W</th>
      <th scope="col-1">T</th>
      <th scope="col-1">F</th>
      <th scope="col-1">S</th>
      <th scope="col-1">S</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      {{#each this.tableArray}}
      {{#if this}}
      <td><div class="circle fill-circle"></div></td>
      {{else}}
      <td><div class="circle"></div></td>
      {{/if}}
      {{/each}}
    </tr>
  </tbody>
</table>
</div>
</section>

{{!-- CREATING A TABLE FOR EACH OTHER USER IN THE GROUP --}}
{{#each user}}
    {{this.username}}
    <table class="table caption-top w-50 weekly-progress" >
  <thead>
    <tr>
      <th scope="col-1">M</th>
      <th scope="col-1">T</th>
      <th scope="col-1">W</th>
      <th scope="col-1">T</th>
      <th scope="col-1">F</th>
      <th scope="col-1">S</th>
      <th scope="col-1">S</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      {{#each this.tableArray}}
      {{#if this}}
      <td><div class="circle fill-circle"></div></td>
      {{else}}
      <td><div class="circle"></div></td>
      {{/if}}
      {{/each}}
    </tr>
  </tbody>
</table>
</div>
</section>

{{/each}}